{
  "name": "agitcms",
  "version": "2.0.1",
  "description": "A headless CMS for markdown blogs",
  "bin": "cli/index.js",
  "scripts": {
    "build": "npm run build:renderer && npm run build:server",
    "renderer": "webpack-dev-server --mode development --config webpack.renderer.js --port 3131",
    "server": "cd server && nodemon -e ts --exec 'ts-node -r tsconfig-paths/register index.ts'",
    "dev": "concurrently \"npm run renderer\" \"wait-on http://localhost:3131 && npm run server\"",
    "lint": "eslint renderer server shared",
    "build:renderer": "webpack --config webpack.renderer.js --mode production",
    "build:server": "webpack --config webpack.server.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xsuk/agitcms.git"
  },
  "keywords": [
    "Headless CMS",
    "Hugo",
    "Jekyll",
    "Static Sites Generator",
    "Markdown",
    "CodeMirror"
  ],
  "author": "0xsuk <iam0xsuk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0xsuk/agitcms/issues"
  },
  "homepage": "https://0xsuk.github.io/agitcms/overview",
  "dependencies": {
    "@codemirror/commands": "^6.0.0",
    "@codemirror/lang-javascript": "^6.0.2",
    "@codemirror/lang-markdown": "^6.0.0",
    "@codemirror/language": "^6.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@date-io/date-fns": "^2.14.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@iarna/toml": "^2.2.5",
    "@lezer/highlight": "^1.0.0",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.2",
    "@mui/x-date-pickers": "^5.0.2",
    "buffer": "^6.0.3",
    "codemirror": "^6.0.1",
    "cors": "^2.8.5",
    "css-loader": "^6.7.1",
    "express": "^4.18.1",
    "github-markdown-css": "^5.1.0",
    "gray-matter": "^4.0.3",
    "node-pty": "^0.10.1",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.1",
